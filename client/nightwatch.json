{
	"src_folders": [
		"spec/integration"
	],
	"output_folder": "spec/reports",
	"custom_commands_path": "",
	"custom_assertions_path": "",
	"page_objects_path": "./spec/support/integration/pageObjects/",
	"globals_path": "./spec/support/integration/globalsModule.js",
	"selenium": {
		"start_process": true,
		"server_path": "./node_modules/selenium-standalone/.selenium/selenium-server/2.45.0-server.jar",
		"log_path": "",
		"host": "127.0.0.1",
		"port": 4444,
		"cli_args": {
			"webdriver.chrome.driver": "node_modules/selenium-standalone/.selenium/chromedriver/2.14-x64-chromedriver",
			"webdriver.ie.driver": ""
		}
	},
	"test_settings": {
		"default": {
			"launch_url": "http://localhost:9010",
			"selenium_port": 4444,
			"selenium_host": "localhost",
			"silent": true,
			"screenshots": {
				"enabled": true,
				"path": "./tmp/selenium"
			},
			"desiredCapabilities": {
				"browserName": "firefox",
				"javascriptEnabled": true,
				"acceptSslCerts": true
			}
		},
		"chrome": {
			"desiredCapabilities": {
				"browserName": "chrome",
				"javascriptEnabled": true,
				"acceptSslCerts": true
			}
		}
	}
}
